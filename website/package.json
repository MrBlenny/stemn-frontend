{
  "name": "Stemn",
  "version": "1.0.0",
  "description": "A workflow for Angular made with Webpack",
  "scripts": {
    "build": "better-npm-run build",
    "server": "better-npm-run server",
    "server:test": "better-npm-run server:test",
    "test": "karma start",
    "test-watch": "karma start --auto-watch --no-single-run"
  },
  "betterScripts": {
    "build": {
      "command": "rimraf dist && webpack --bail --progress --profile",
      "env": {
        "ENV_TYPE": "production",
        "API_SERVER": "https://stemn.com"
      }
    },
    "server": {
      "command": "webpack-dev-server --max-old-space-size=4096 --history-api-fallback --inline --progress --display-error-details",
      "env": {
        "ENV_TYPE": "development",
        "API_SERVER": "http://localhost:3000"
      }
    },
    "server:test": {
      "command": "webpack-dev-server --max-old-space-size=4096 --history-api-fallback --inline --progress --display-error-details",
      "env": {
        "ENV_TYPE": "development",
        "API_SERVER": "http://35.162.145.41"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Foxandxss/angular-webpack-workflow.git"
  },
  "author": "David Revay",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Foxandxss/angular-webpack-workflow/issues"
  },
  "homepage": "https://github.com/Foxandxss/angular-webpack-workflow",
  "dependencies": {
