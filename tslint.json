{
  "extends": [
    "@stemn/tslint-config",
    "tslint-react"
  ],
  "rules": {
    "no-console": false,
    "no-implicit-dependencies": [false, "dev"],
    "ter-max-len": [true, 200],
    "semicolon": [true, "never"],
    "one-variable-per-declaration": false,
    "no-default-export": false, // Allow temporarily...

    // JSX
    "jsx-no-multiline-js": false,
    "jsx-no-lambda": false
  }
}
