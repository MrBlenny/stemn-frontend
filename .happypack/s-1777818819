  };
}

function fetchTimeline(_ref3) {
  var projectId = _ref3.projectId;
  var fileId = _ref3.fileId;

  return {
    type: 'TIMELINE/FETCH_TIMELINE',
    http: true,
    payload: {
      method: 'GET',
      url: '/api/v1/sync/timeline/' + projectId,
      params: {
        types: ['revisions', 'commits'],
        file: fileId
      }
    },
    meta: {
      cacheKey: fileId ? fileId : projectId
    }
  };
}